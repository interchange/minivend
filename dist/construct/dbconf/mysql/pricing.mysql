Database  pricing  COLUMN_DEF   "q5=CHAR(12)"
Database  pricing  COLUMN_DEF   "q10=CHAR(12)"
Database  pricing  COLUMN_DEF   "q25=CHAR(12)"
Database  pricing  COLUMN_DEF   "q100=CHAR(12)"
