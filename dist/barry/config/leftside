[loop
	prefix=box
	search="
		fi=area
		st=db
		tf=sort
		ac=0
		ac=0
		co=yes
		
		sf=sel
			op=eq
	[if scratch left_bar]
		se=[scratch left_bar][set left_bar][/set]
	[else]
		se=left
	[/else]
	[/if]

		sf=which_page
			op=rm
	[if scratch page_class]
		se=all|[scratch page_class][set page_class][/set]
	[else]
		se=all|@@MV_PAGE@@
	[/else]
	[/if]
	"]
<!-- begin area loop -->

